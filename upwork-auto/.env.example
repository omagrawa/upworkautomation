# Upwork Automation Project - Environment Variables
# Copy this file to .env and fill in your actual values

# Apify Integration
APIFY_TOKEN=
APIFY_USERNAME=
APIFY_SUBMIT_ACTOR_ID=<username>~apify-submit

# Upwork Authentication
UPWORK_SESSION_COOKIE=

# OpenAI Integration
OPENAI_API_KEY=

# Storage Configuration
STORAGE_TARGET=sheets|postgres
SHEETS_ID=

# PostgreSQL Database Configuration
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=
PGDATABASE=n8n

# n8n Configuration
N8N_WEBHOOK_URL=http://localhost:5678

# Auto-submission Configuration
AUTO_SUBMIT=false
