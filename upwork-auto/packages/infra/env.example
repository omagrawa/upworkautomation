# Database Configuration
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=your_secure_password_here
POSTGRES_PORT=5432

# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_WEBHOOK_URL=http://localhost:5678
N8N_API_KEY=your_n8n_api_key_here

# n8n Security
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# n8n Logging
N8N_LOG_LEVEL=info
N8N_METRICS=false

# Adminer Configuration
ADMINER_PORT=8080

# Redis Configuration (Optional)
REDIS_PASSWORD=redis_password_here
REDIS_PORT=6379

# Nginx Configuration (Optional - for production)
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# Timezone
TIMEZONE=UTC

# Apify Integration
APIFY_TOKEN=your_apify_token_here
APIFY_ACTOR_SCRAPER_ID=your_scraper_actor_id
APIFY_ACTOR_SUBMIT_ID=your_submit_actor_id

# Google Sheets Integration
GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here
GOOGLE_SHEETS_CREDENTIALS_PATH=./credentials/google-sheets.json

# Upwork Credentials
UPWORK_USERNAME=your_upwork_username
UPWORK_PASSWORD=your_upwork_password
