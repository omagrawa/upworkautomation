{
  "name": "@upwork-auto/apify-submit",
  "version": "1.0.0",
  "description": "Apify actor for automated Upwork job proposal submission using Playwright",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "dev": "node --watch src/main.js",
    "build": "echo 'No build step required'",
    "clean": "rm -rf .apify dist"
  },
  "dependencies": {
    "apify": "^3.1.10",
    "playwright": "^1.40.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "apify": {
    "actorSpecification": 1,
    "name": "upwork-job-submitter",
    "title": "Upwork Job Submitter",
    "description": "Automatically submits proposals to Upwork jobs",
    "version": "1.0.0",
    "meta": {
      "templateId": "playwright-js"
    },
    "input": "./input_schema.json",
    "dockerfile": "./Dockerfile"
  }
}
